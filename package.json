{
    "name": "skeleton-internal-front",
    "version": "2.0.0",
    "scripts": {
        "test": "./node_modules/.bin/ng test",
        "e2e": "./node_modules/.bin/ng e2e",
        "mock": "./node_modules/.bin/json-server --watch mocks/db.json --delay 700",
        "stylelint:fix": "./node_modules/.bin/stylelint 'src/**/**/*.scss' --fix",
        "stylelint": "./node_modules/.bin/stylelint 'src/**/**/*.scss'",
        "eslint:fix": "./node_modules/.bin/eslint 'src/app/**/*.ts' --fix",
        "eslint": "./node_modules/.bin/eslint 'src/app/**/*.ts' --quiet",
        "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
        "lint": "npm run eslint && npm run stylelint",
        "clear": "rm -rf dist",
        "serve": "ng serve --disable-host-check --port 4200 --live-reload false",
        "check-version": "./node_modules/.bin/check-node-version --node 20.10.0 --npm 10.2.3",
        "pre-build": "npm run check-version && npm run clear",
        "dev": "./node_modules/.bin/ng serve --open",
        "watch": "./node_modules/.bin/ng build --output-path dist --watch",
        "start": "npm run watch",
        "build": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve",
        "build-www": "npm run pre-build && ./node_modules/.bin/ng build --configuration=production",
        "build-demo": "npm run pre-build && ./node_modules/.bin/ng build --configuration=demo",
        "build-homo": "npm run pre-build && ./node_modules/.bin/ng build --configuration=homologacao",
        "build-deve-um": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-um",
        "build-deve-dois": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-dois",
        "build-deve-tres": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-tres",
        "build-deve-quatro": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-quatro",
        "build-deve-cinco": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-cinco",
        "build-deve-seis": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-seis",
        "build-deve-sete": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-sete",
        "build-deve-oito": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-oito",
        "build-deve-nove": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-nove",
        "build-deve-dez": "npm run pre-build && ./node_modules/.bin/ng build --configuration=deve-dez",
        "prepare": "husky install"
    },
    "private": true,
    "engines": {
        "node": ">=20.10.0"
    },
    "dependencies": {
        "@angular-builders/custom-webpack": "^17",
        "@angular-devkit/build-angular": "^17.0.9",
        "@angular/animations": "^17.0.8",
        "@angular/cli": "^17.0.9",
        "@angular/common": "^17.0.8",
        "@angular/compiler": "^17.0.8",
        "@angular/compiler-cli": "^17.0.8",
        "@angular/core": "^17.0.8",
        "@angular/forms": "^17.0.8",
        "@angular/language-service": "^17.0.8",
        "@angular/platform-browser": "^17.0.8",
        "@angular/platform-browser-dynamic": "^17.0.8",
        "@angular/router": "^17.0.8",
        "@brazilian-utils/is-valid-cnpj": "^1.0.0-alpha.3",
        "@brazilian-utils/is-valid-cpf": "^1.0.0-alpha.3",
        "@sentry/angular-ivy": "^7.92.0",
        "check-node-version": "^4.2.1",
        "husky": "^8.0.3",
        "js-sha512": "^0.8.0",
        "lib-vox-shared-codes": "git+ssh://git@gitlab.voxtecnologia.com.br/vox/bibliotecas/front-end/lib-vox-shared-codes.git#1.2.4",
        "lib-vox-ui": "git+ssh://git@gitlab.voxtecnologia.com.br/vox/bibliotecas/front-end/lib-vox-ui.git#3.26.10",
        "ngx-mask": "^12.0.0",
        "ngx-skeleton-loader": "^8.1.0",
        "rxjs": "^7.8.1",
        "single-spa": ">= 5.4",
        "single-spa-angular": "^9.0.1",
        "zone.js": "~0.14.2"
    },
    "devDependencies": {
        "@types/jasmine": "~5.1.4",
        "@types/jasminewd2": "^2.0.13",
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.2",
        "jasmine-core": "~5.1.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "^6.4.2",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "postcss": "^8.4.32",
        "postcss-loader": "^7.3.4",
        "postcss-scss": "^4.0.9",
        "prettier": "^3.1.1",
        "prettier-eslint": "^16.2.0",
        "stylelint": "^15.11.0",
        "stylelint-config-sass-guidelines": "^10.0.0",
        "ts-node": "~10.9.2",
        "tslib": "^2.6.2",
        "typescript": "~5.2.2",
        "util": "^0.12.4"
    }
}
