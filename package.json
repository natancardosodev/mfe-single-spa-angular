{
    "name": "skeleton-interno-angular",
    "version": "2.0.0",
    "scripts": {
        "test": "ng test",
        "e2e": "ng e2e",
        "mock": "./node_modules/.bin/json-server --watch db/db.json",
        "stylelint:fix": "./node_modules/.bin/stylelint 'src/**/**/*.scss' --fix",
        "stylelint": "./node_modules/.bin/stylelint 'src/**/**/*.scss'",
        "eslint:fix": "./node_modules/.bin/eslint src/app/** --ext .ts --fix",
        "eslint": "./node_modules/.bin/eslint src/app/** --ext .ts --quiet",
        "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
        "lint": "npm run eslint && npm run stylelint",
        "clear-proj": "rm -rf dist",
        "check-version": "./node_modules/.bin/check-node-version --node 12 --npm 5.10.0",
        "pre-build": "npm run check-version && npm run clear-proj && npm run lint",
        "dev": "ng serve --open",
        "watch": "ng build --output-path dist --watch",
        "start": "npm run watch",
        "build": "npm run pre-build && ng build --configuration=deve",
        "build-www": "npm run pre-build && ng build --configuration=production",
        "build-demo": "npm run pre-build && ng build --configuration=demo",
        "build-homo": "npm run pre-build && ng build --configuration=homologacao",
        "build-deve-um": "npm run pre-build && ng build --configuration=deve-um",
        "build-deve-dois": "npm run pre-build && ng build --configuration=deve-dois",
        "build-deve-tres": "npm run pre-build && ng build --configuration=deve-tres",
        "build-deve-quatro": "npm run pre-build && ng build --configuration=deve-quatro",
        "build-deve-cinco": "npm run pre-build && ng build --configuration=deve-cinco",
        "build-deve-seis": "npm run pre-build && ng build --configuration=deve-seis",
        "build-deve-sete": "npm run pre-build && ng build --configuration=deve-sete",
        "build-deve-oito": "npm run pre-build && ng build --configuration=deve-oito",
        "build-deve-nove": "npm run pre-build && ng build --configuration=deve-nove",
        "build-deve-dez": "npm run pre-build && ng build --configuration=deve-dez"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~12.2.16",
        "@angular/common": "~12.2.16",
        "@angular/compiler": "~12.2.16",
        "@angular/core": "~12.2.16",
        "@angular/forms": "~12.2.16",
        "@angular/platform-browser": "~12.2.16",
        "@angular/platform-browser-dynamic": "~12.2.16",
        "@angular/router": "~12.2.16",
        "@brazilian-utils/is-valid-cnpj": "^1.0.0-alpha.3",
        "@brazilian-utils/is-valid-cpf": "^1.0.0-alpha.3",
        "@fortawesome/free-solid-svg-icons": "^5.13.1",
        "ag-grid": "^16.0.1",
        "ag-grid-angular": "^16.0.0",
        "angular2-text-mask": "^9.0.0",
        "grid": "git+ssh://git@gitlab.voxtecnologia.com.br/vox/bibliotecas/front-end/lib-vox-grid.git#1.7.1",
        "js-sha512": "^0.8.0",
        "lib-ui-interno": "git+ssh://git@gitlab.voxtecnologia.com.br:vox/bibliotecas/front-end/lib-ui-interno.git#2.5.1",
        "ngx-bootstrap": "~5.5.0",
        "ngx-currency": "^2.3.3",
        "rxjs": "^6.5.3",
        "stylelint": "^13.6.1",
        "stylelint-config-sass-guidelines": "^7.0.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.2.16",
        "@angular/cli": "~12.2.16",
        "@angular/compiler-cli": "~12.2.16",
        "@angular/language-service": "~12.2.16",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/eslint-plugin-tslint": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "check-node-version": "^4.0.3",
        "eslint": "^7.7.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "json-server": "^0.16.3",
        "karma": "~6.3.17",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "prettier": "^2.1.1",
        "prettier-eslint": "^11.0.0",
        "protractor": "~7.0.0",
        "ts-node": "~7.0.0",
        "typescript": "~4.3.5",
        "util": "^0.12.4"
    }
}
